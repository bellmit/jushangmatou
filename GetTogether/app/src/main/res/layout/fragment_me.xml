<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.tem.gettogether.view.BaseScrollView
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ll_myhead"
                android:layout_width="match_parent"
                android:background="@drawable/plan_progress_one"
                android:orientation="vertical"
                android:layout_height="190dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:layout_marginTop="35dp"
                    android:gravity="center_vertical"
                    android:paddingEnd="15dp"
                    android:orientation="horizontal">
                    <RelativeLayout
                        android:layout_width="59dp"
                        android:layout_height="59dp"
                        android:layout_marginEnd="10dp">
                        <com.tem.gettogether.view.CircularImage
                            android:id="@+id/iv_head"
                            android:layout_width="59dp"
                            android:layout_height="59dp"
                            android:src="@drawable/img12x"/>
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_height="wrap_content"
                            android:src="@drawable/my_huiyuan"/>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/tv_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="南有嘉人"
                            android:textColor="@color/white"
                            android:textSize="19dp"/>
                        <TextView
                            android:id="@+id/tv_hy_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:visibility="gone"
                            android:layout_marginTop="5dp"
                            android:textColor="@color/white"
                            android:textSize="12dp"/>

                        <LinearLayout
                            android:id="@+id/ll_dprz"
                            android:layout_width="wrap_content"
                            android:layout_height="21dp"
                            android:gravity="center_vertical"
                            android:layout_marginTop="10dp"
                            android:paddingStart="8dp"
                            android:paddingEnd="8dp"
                            android:background="@drawable/my_renzheng"
                            android:orientation="horizontal">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/renzheng" />
                            <TextView
                                android:id="@+id/tv_shopZZ"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="我的店铺认证"
                                android:layout_marginStart="4dp"
                                android:layout_marginEnd="4dp"
                                android:textColor="@color/white"
                                android:textSize="11dp"/>
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/rz_more" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/ll_scj"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/tv_scNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="13dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/shoucangjia"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/white"
                            android:textSize="13dp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll_zj"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/tv_zjNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="13dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/zuji"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/white"
                            android:textSize="13dp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll_qb"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/tv_qb_num"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="13dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/qianbao"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/white"
                            android:textSize="13dp"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:id="@+id/tv_setting"
                android:layout_width="wrap_content"
                android:padding="15dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="29dp"
                android:text="@string/settings"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="15dp"/>
            <TextView
                android:id="@+id/tv_bg"
                android:layout_width="match_parent"
                android:background="@color/my_kb_bg"
                android:layout_below="@+id/ll_myhead"
                android:layout_height="70dp" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginTop="178dp"
                android:layout_marginEnd="13dp"
                android:gravity="center_vertical"
                android:layout_marginStart="13dp"
                android:background="@drawable/my_zhuangtai_bg">

                <TextView
                    android:id="@+id/tv_all"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:drawableTop="@drawable/my_zt_all"
                    android:drawablePadding="10dp"
                    android:textColor="@color/text6"
                    android:text="@string/quanbu"
                    android:textSize="12dp"/>

                <TextView
                    android:id="@+id/tv_dfk"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:drawablePadding="10dp"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:drawableTop="@drawable/my_zt_all"
                    android:gravity="center"
                    android:text="@string/daifukuan"
                    android:textColor="@color/text6"
                    android:textSize="12dp" />
                <TextView
                    android:id="@+id/tv_dfh"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:drawableTop="@drawable/my_zt_all"
                    android:drawablePadding="10dp"
                    android:textColor="@color/text6"
                    android:text="@string/daifahuo"
                    android:textSize="12dp"/>
                <TextView
                    android:id="@+id/tv_dsh"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:drawableTop="@drawable/my_zt_all"
                    android:drawablePadding="10dp"
                    android:textColor="@color/text6"
                    android:text="@string/daishouhuo"
                    android:textSize="12dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_bg"
                android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="16dp"
                android:paddingEnd="16dp"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/rl_fabupingji"
                    android:layout_width="match_parent"
                    android:visibility="gone"
                    android:layout_height="51dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="发布评价"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text3"
                        android:textSize="14dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/right_more"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_my_message"
                    android:layout_width="match_parent"
                    android:layout_height="51dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/message"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text3"
                        android:textSize="14dp"/>

                    <com.tem.gettogether.view.DragPointView
                        android:id="@+id/seal_num"
                        android:layout_width="19dp"
                        android:layout_height="19dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginTop="3dp"
                        android:textColor="@android:color/white"
                        android:textSize="12sp"
                        android:visibility="invisible" />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/right_more"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/line"/>
                <RelativeLayout
                    android:id="@+id/rl_ksbh"
                    android:layout_width="match_parent"
                    android:layout_height="51dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ksbh"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text3"
                        android:textSize="14dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toLeftOf="@+id/iv_more"
                        android:layout_marginEnd="14dp"
                        android:visibility="invisible"
                        android:text="真实反馈，体验更好"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text9"
                        android:textSize="14dp"/>
                    <ImageView
                        android:id="@+id/iv_more"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/right_more"/>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/line"/>

                <RelativeLayout
                    android:id="@+id/rl_dzgl"
                    android:layout_width="match_parent"
                    android:layout_height="51dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/address"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text3"
                        android:textSize="14dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/right_more"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/line"/>
                <RelativeLayout
                    android:id="@+id/rl_sprz"
                    android:layout_width="match_parent"
                    android:layout_height="51dp">
                    <TextView
                        android:id="@+id/tv_shop_RZ"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/shopRZ"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text3"
                        android:textSize="14dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/right_more"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/line"/>
                <RelativeLayout
                    android:id="@+id/rl_sprz_new"
                    android:layout_width="match_parent"
                    android:visibility="gone"
                    android:layout_height="51dp">
                    <TextView
                        android:id="@+id/tv_shop_RZ_new"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="发布动态"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text3"
                        android:textSize="14dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/right_more"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_dpgl"
                    android:layout_width="match_parent"
                    android:visibility="gone"
                    android:layout_height="51dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="店铺管理"
                        android:layout_centerVertical="true"
                        android:textColor="@color/text3"
                        android:textSize="14dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/right_more"/>
                </RelativeLayout>

            </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:background="@color/my_kb_bg"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:orientation="vertical">
                    <RelativeLayout
                        android:id="@+id/rl_zxkf"
                        android:layout_width="match_parent"
                        android:layout_height="51dp">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/zxkf"
                            android:layout_centerVertical="true"
                            android:textColor="@color/text3"
                            android:textSize="14dp"/>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true"
                            android:src="@drawable/right_more"/>
                    </RelativeLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/line"/>
                    <RelativeLayout
                        android:id="@+id/rl_tdyj"
                        android:layout_width="match_parent"
                        android:layout_height="51dp">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tdyj"
                            android:layout_centerVertical="true"
                            android:textColor="@color/text3"
                            android:textSize="14dp"/>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true"
                            android:src="@drawable/right_more"/>
                    </RelativeLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/line"/>
                    <RelativeLayout
                        android:id="@+id/rl_gywm"
                        android:layout_width="match_parent"
                        android:layout_height="51dp">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/gywm"
                            android:layout_centerVertical="true"
                            android:textColor="@color/text3"
                            android:textSize="14dp"/>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true"
                            android:src="@drawable/right_more"/>
                    </RelativeLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/line"/>
                </LinearLayout>

            </LinearLayout>
        </RelativeLayout>
    </com.tem.gettogether.view.BaseScrollView>

</LinearLayout>
